{
  "name": "janczizikow-portfolio",
  "version": "1.0.0",
  "repository": "git@github.com:janczizikow/gatsby-sleek.git",
  "author": "Jan Czizikow <jan.czizikow@gmail.com>",
  "license": "MIT",
  "private": false,
  "lint-staged": {
    "src/**/*.{js,jsx,json}": [
      "eslint --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "stylelint --config=.stylelintrc --fix",
      "prettier --config .prettierrc --write",
      "git add"
    ]
  },
  "scripts": {
    "clean": "gatsby clean",
    "start": "yarn run dev",
    "develop": "yarn run dev",
    "dev": "GATSBY_GRAPHQL_IDE=playground gatsby clean && gatsby develop",
    "build": "gatsby clean && gatsby build",
    "serve": "gatsby serve",
    "format": "prettier --write 'src/**/*.{js,jsx}'",
    "lint:css": "stylelint --config=.stylelintrc --fix 'src/**/*.{js, jsx}'; exit 0",
    "lint:js": "eslint --config=.eslintrc --fix 'src/**/*.{js,jsx}'; exit 0",
    "lint": "yarn lint:css && yarn lint:js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "axios": "^0.19.0",
    "body-scroll-lock": "^2.6.4",
    "clean-tag": "^2.0.3",
    "dotenv": "^8.0.0",
    "emotion-theming": "^10.0.14",
    "facepaint": "^1.2.1",
    "formik": "^1.5.8",
    "gatsby": "^2.13.41",
    "gatsby-image": "^2.2.7",
    "gatsby-plugin-canonical-urls": "^2.1.2",
    "gatsby-plugin-emotion": "^4.1.2",
    "gatsby-plugin-favicon": "^3.1.6",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-google-tagmanager": "^2.1.4",
    "gatsby-plugin-layout": "^1.1.2",
    "gatsby-plugin-manifest": "^2.2.4",
    "gatsby-plugin-netlify": "^2.1.3",
    "gatsby-plugin-netlify-cache": "^1.2.0",
    "gatsby-plugin-nprogress": "^2.1.2",
    "gatsby-plugin-offline": "^2.2.4",
    "gatsby-plugin-react-helmet": "^3.1.2",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sharp": "^2.2.9",
    "gatsby-plugin-sitemap": "^2.2.3",
    "gatsby-plugin-typography": "^2.3.2",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-source-filesystem": "^2.1.6",
    "gatsby-transformer-sharp": "^2.2.4",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-dom": "^16.8.6",
    "react-headroom": "^2.2.8",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.7.0",
    "react-modal": "^3.9.1",
    "react-pose": "^4.0.8",
    "react-typography": "^0.16.19",
    "styled-system": "^4.1.0",
    "typography": "^0.16.19",
    "typography-theme-bootstrap": "^0.16.19",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "babel-preset-gatsby": "^0.2.8",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-emotion": "^10.0.14",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-react-hooks": "^1.6.1",
    "husky": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-emotion": "^10.0.14",
    "lint-staged": "^9.2.1",
    "prettier": "^1.18.2",
    "stylelint": "^9.5.0",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/jest-preprocess.js"
    },
    "testRegex": "/(.*(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "testPathIgnorePatterns": [
      "node_modules",
      ".cache"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "jest-emotion/serializer"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ]
  }
}
